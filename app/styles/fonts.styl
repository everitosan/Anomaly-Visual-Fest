@import "variables"

@font-face
    font-family 'AVF'
    src url(icomoon-font-path + '/AVF.eot?pg5d3l')
    src url(icomoon-font-path + '/AVF.eot?pg5d3l#iefix') format('embedded-opentype'), url(icomoon-font-path + '/AVF.ttf?pg5d3l') format('truetype'), url(icomoon-font-path + '/AVF.woff?pg5d3l') format('woff'), url(icomoon-font-path + '/AVF.svg?pg5d3l#AVF') format('svg')
    font-weight normal
    font-style normal
    
@font-face
  font-family 'bebas'
  src url(icomoon-font-path +'/bebasneue_bold-webfont.eot')
  src url(icomoon-font-path +'/bebasneue_bold-webfont.eot?#iefix') format('embedded-opentype'),
         url(icomoon-font-path +'/bebasneue_bold-webfont.woff2') format('woff2'),
         url(icomoon-font-path +'/bebasneue_bold-webfont.woff') format('woff'),
         url(icomoon-font-path +'/bebasneue_bold-webfont.ttf') format('truetype'),
         url(icomoon-font-path +'/bebasneue_bold-webfont.svg#bebas_neuebold') format('svg')
  font-weight normal
  font-style normal

@font-face
  font-family 'league'
  src url(icomoon-font-path +'/leaguegothic-regular-webfont.eot')
  src url(icomoon-font-path +'/leaguegothic-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url(icomoon-font-path +'/leaguegothic-regular-webfont.woff2') format('woff2'),
         url(icomoon-font-path +'/leaguegothic-regular-webfont.woff') format('woff'),
         url(icomoon-font-path +'/leaguegothic-regular-webfont.ttf') format('truetype'),
         url(icomoon-font-path +'/leaguegothic-regular-webfont.svg#league_gothicregular') format('svg')
  font-weight normal
  font-style normal


@font-face
    font-family 'InputMonoCompressed-Bold'
    src url(icomoon-font-path +'/InputMonoCompressed-Bold.eot')
    src local('☺'), url(icomoon-font-path +'/InputMonoCompressed-Bold.woff') format('woff'), url(icomoon-font-path +'/InputMonoCompressed-Bold.ttf') format('truetype'), url(icomoon-font-path +'/InputMonoCompressed-Bold.svg') format('svg')
    font-weight normal
    font-style normal
    
@font-face
    font-family 'InputMonoCompressed-Light'
    src url(icomoon-font-path +'/InputMonoCompressed-Light.eot')
    src local('☺'), url(icomoon-font-path +'/InputMonoCompressed-Light.woff') format('woff'), url(icomoon-font-path +'/InputMonoCompressed-Light.ttf') format('truetype'), url(icomoon-font-path +'/InputMonoCompressed-Light.svg') format('svg')
    font-weight normal
    font-style normal
    
@font-face
    font-family 'InputMonoCompressed-Regular'
    src url(icomoon-font-path +'/InputMonoCompressed-Regular.eot')
    src local('☺'), url(icomoon-font-path +'/InputMonoCompressed-Regular.woff') format('woff'), url(icomoon-font-path +'/InputMonoCompressed-Regular.ttf') format('truetype'), url(icomoon-font-path +'/InputMonoCompressed-Regular.svg') format('svg')
    font-weight normal
    font-style normal

[class^="icon-"], [class*=" icon-"]
    font-family 'AVF' !important
    speak none
    font-style normal
    font-weight normal
    font-variant normal
    text-transform none
    line-height 1

    /* Better Font Rendering =========== */
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale

.icon-facebook
    &:before
        content icon-facebook
.icon-instagram
    &:before
        content icon-instagram
.icon-logo1
    &:before
        content icon-logo1
.icon-logo2
    &:before
        content icon-logo2
.icon-menu
    &:before
        content icon-menu
.icon-pointer
    &:before
        content icon-pointer
.icon-twitter
    &:before
        content icon-twitter
.icon-x
    &:before
        content icon-x

